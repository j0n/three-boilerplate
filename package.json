{
  "name": "three-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "eslint:vim": "eslint --report-unused-disable-directives",
    "build:css": "cssjoin app/styles/main.css | autoprefixer -b 'last 2 versions' | cssmin > dist/main.css",
    "build:html": "catw 'app/index.html' -o dist/index.html -v",
    "serve": "http-server -p 9999 dist/",
    "build:js": "browserify -t [ babelify --presets [ es2015 ]] app/scripts/main.js | uglifyjs > dist/main.js",
    "watch:js": "watchify -t browserify-shader app/scripts/main.js -o dist/main.js -vd",
    "watch:css": "nodemon --watch app/styles/ -e css -x \"npm run build:css\"",
    "watch": "mkdir -p & npm run serve & npm run watch:js & npm run build:html",
    "build": "mkdir -p dist & npm run build:html & npm run build:js",
    "lint": "esw -c ./.eslintrc app/scripts -w",
    "lint:fix": "eslint app/scripts/ --fix",
    "dev": "npm run watch",
    "webpack:test": "webpack",
    "webpack:start": "webpack-dev-server --mode development"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "author": "Jon",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-glslify": "^2.0.0",
    "brfs": "^2.0.2",
    "chroma-js": "^2.0.6",
    "gaussian": "^1.1.0",
    "glsl-film-grain": "^1.0.4",
    "glsl-noise": "0.0.0",
    "glsl-noise-simplex": "0.0.2",
    "glsl-random": "0.0.5",
    "ify-loader": "^1.1.0",
    "random": "^2.1.1",
    "three": "^0.99.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "autoprefixer": "^5.1.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^8.0.6",
    "babelify": "^10.0.0",
    "browserify": "^14.1.0",
    "browserify-shader": "^0.1.2",
    "cssjoin": "^0.3.1",
    "cssmin": "^0.4.3",
    "fps": "0.0.3",
    "gl-shader": "^4.2.1",
    "glslify": "^7.0.0",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "nodemon": "^1.19.4",
    "orbit-controls": "^1.1.2",
    "raw-loader": "^3.1.0",
    "shader-loader": "^1.3.1",
    "three-orbit-controls": "^82.1.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.11.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "repository": {}
}
